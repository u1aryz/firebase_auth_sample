{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "outDir": "public",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "removeComments": true,
    "jsx": "react"
  }
}
